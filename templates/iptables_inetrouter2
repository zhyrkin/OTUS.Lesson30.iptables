*nat
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]
-A PREROUTING -p tcp -m tcp --dport 8080 -j DNAT --to-destination 192.168.0.2:80
-A POSTROUTING -o ens19 -j MASQUERADE
-A POSTROUTING -d 192.168.0.2/32 -o ens19 -p tcp -m tcp --dport 80 -j SNAT --to-source 192.168.255.6:8080
COMMIT